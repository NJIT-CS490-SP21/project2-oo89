{
  "root": true,
  "extends": "airbnb",
  "env": {
    "node": true,
    "browser": true
  },
  "ignorePatterns": ["App.test.js", "AppDE.js","Board.test.js","index.js","NextWinner.test.js","reportWebVitals.js","setupTests.js","ListItem.js"],
  "rules": {
    "no-console": "off",
    "import/no-unresolved": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/no-array-index-key": "off",
    "no-nested-ternary": "off",
    "react/prop-types": "off"
    
    
  },
  
  "parser": "babel-eslint",
  "parserOptions": {
  "sourceType": "module",
  "allowImportExportEverywhere": true
  }

}
